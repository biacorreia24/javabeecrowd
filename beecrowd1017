import java.io.IOException;
import java.util.Scanner;

public class Main {
 
    public static void main(String[] args) throws IOException {
 
        Scanner ler = new Scanner(System.in);

        int A = ler.nextInt();
        int B = ler.nextInt();
        double km = (A * B);
        double l = km / 12;

        System.out.printf("%.3f%n" , l);
 
    }
 
}
